<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

    <declare-styleable name="LoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension" />
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension" />
        <attr name="indicatorName" format="string" />
        <attr name="indicatorColor" format="color" />
    </declare-styleable>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsTabTextSize" format="dimension" />
        <attr name="pstsDefTextColor" format="color" />
        <attr name="pstsSelTextColor" format="color" />
        <attr name="pstsIndicatorPaddingLeftRight" format="dimension" />
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsDividerWidth" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
    </declare-styleable>

    <!--圆角自定义嵌套layout-->
    <declare-styleable name="ClickImageView">
        <attr name="colorRipple" />
        <attr name="haveRipple" />
        <attr name="rippleOver" format="boolean" />
        <attr name="haveFilter" format="boolean" />
        <attr name="filterLightNumber" format="float|reference" />
        <attr name="filterLightOrDark" format="boolean" />
    </declare-styleable>

    <!--圆角嵌套layout-->
    <declare-styleable name="RecShapeFrameLayout">
        <attr name="heightWidthRatio" format="float" />
        <attr name="baseOnWidthOrHeight" format="boolean" />
        <attr name="colorRipple" />
        <attr name="haveRipple" />

        <attr name="colorFill" />
        <attr name="radiusCorner" />
        <attr name="radiusTopLeft" />
        <attr name="radiusTopRight" />
        <attr name="radiusBottomRight" />
        <attr name="radiusBottomLeft" />
        <attr name="radiusGradient" />
        <attr name="colorStart" />
        <attr name="colorCenter" />
        <attr name="colorEnd" />
        <attr name="centerX" />
        <attr name="centerY" />
        <attr name="angle" format="integer" />
        <attr name="strokeWidth" />
        <attr name="strokePaddingLeft" />
        <attr name="strokePaddingTop" />
        <attr name="strokePaddingRight" />
        <attr name="strokePaddingBottom" />
        <attr name="strokeColor" />
        <attr name="strokeDashWidth" />
        <attr name="strokeDashGap" />

        <attr name="orientationGradient" format="integer" />
        <attr name="gradientType" format="integer" />
        <attr name="shapeType" format="integer" />

    </declare-styleable>

    <!--圆角嵌套textView-->
    <declare-styleable name="RecShapeTextView">
        <attr name="heightWidthRatio" />
        <attr name="tvBaseOnWidthOrHeight" format="boolean" />
        <attr name="colorRipple" format="color" />
        <attr name="haveRipple" format="boolean" />

        <attr name="colorFill" format="color" />
        <attr name="radiusCorner" format="dimension" />
        <attr name="radiusTopLeft" format="dimension" />
        <attr name="radiusTopRight" format="dimension" />
        <attr name="radiusBottomRight" format="dimension" />
        <attr name="radiusBottomLeft" format="dimension" />
        <attr name="radiusGradient" format="dimension" />
        <attr name="colorStart" format="color" />
        <attr name="colorCenter" format="color" />
        <attr name="colorEnd" format="color" />
        <attr name="centerX" format="color" />
        <attr name="centerY" format="color" />
        <attr name="angle" />
        <attr name="strokeWidth" />
        <attr name="strokePaddingLeft" format="dimension" />
        <attr name="strokePaddingTop" format="dimension" />
        <attr name="strokePaddingRight" format="dimension" />
        <attr name="strokePaddingBottom" format="dimension" />
        <attr name="strokeColor" />
        <attr name="strokeDashWidth" format="dimension" />
        <attr name="strokeDashGap" format="dimension" />

        <attr name="orientationGradient" />
        <attr name="gradientType" />
        <attr name="shapeType" />

    </declare-styleable>

    <!--真正用于解析的属性-->
    <declare-styleable name="RCAttrs">
        <attr name="round_as_circle" format="boolean" />
        <attr name="round_corner" format="dimension" />
        <attr name="round_corner_top_left" format="dimension" />
        <attr name="round_corner_top_right" format="dimension" />
        <attr name="round_corner_bottom_left" format="dimension" />
        <attr name="round_corner_bottom_right" format="dimension" />
        <attr name="stroke_color" format="color" />
        <attr name="stroke_width" format="dimension" />
        <attr name="clip_background" format="boolean" />
    </declare-styleable>

    <!--横向线情录音波纹-->
    <declare-styleable name="LineWaveVoiceView">
        <attr name="voiceLineColor" format="color" />
        <attr name="voiceLineWidth" format="dimension" />
        <attr name="voiceTextColor" format="color" />
        <attr name="voiceTextSize" format="dimension" />
        <attr name="voiceText" format="string" />
    </declare-styleable>

    <!--录音圆形键-->
    <declare-styleable name="RecordLayoutCircle">
        <attr name="circleStrokeColor" format="color" />
        <attr name="circleFillColor" format="color" />
        <attr name="circleRadius" format="dimension" />

    </declare-styleable>

    <!--语音消息竖形线条-->
    <declare-styleable name="AudioWaveView">
        <attr name="waveColor" format="color" />
        <attr name="chunkWidth" format="dimension" />
        <attr name="chunkHeight" format="dimension" />
        <attr name="minChunkHeight" format="dimension" />
        <attr name="chunkRadius" format="dimension" />
        <attr name="chunkSpacing" format="dimension" />
        <attr name="progress" format="float" />
        <attr name="animateExpansion" format="boolean" />
        <attr name="touchable" format="boolean" />
    </declare-styleable>

    <!--录音底部layout中的播放和删除圆形控件-->
    <declare-styleable name="CircleBarView">
        <attr name="progress_color" format="color" />
        <attr name="bg_color" format="color" />
        <attr name="progress_bar_width" format="dimension" />
        <attr name="bg_bar_width" format="dimension" />
        <attr name="start_angle" format="float" />
        <attr name="circleBarView_progress" format="float" />
        <attr name="sweep_angle" format="float" />

    </declare-styleable>

    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean" />
        <attr name="ios" format="boolean" />
        <attr name="leftSwipe" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <!-- 描边宽度-->
        <attr name="border_width" format="dimension"/>
        <!-- 描边颜色-->
        <attr name="border_color" format="color"/>
        <!--圆角大小-->
        <attr name="corner_radius" format="dimension"/>
        <!--左上圆角大小-->
        <attr name="leftTop_corner_radius" format="dimension"/>
        <!--右上圆角大小-->
        <attr name="rightTop_corner_radius" format="dimension"/>
        <!--左下圆角大小-->
        <attr name="leftBottom_corner_radius" format="dimension"/>
        <!--右下圆角大小-->
        <attr name="rightBottom_corner_radius" format="dimension"/>
        <!--图片类型：圆角或者圆形-->
        <attr name="type" format="enum">
            <enum name="oval" value="2"/>
            <enum name="round" value="1"/>
            <enum name="circle" value="0"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="NumberProgressBar">
        <attr name="progress_current" format="integer" />
        <attr name="progress_max" format="integer" />

        <attr name="progress_unreached_color" format="color" />
        <attr name="progress_reached_color" format="color" />

        <attr name="progress_reached_bar_height" format="dimension" />
        <attr name="progress_unreached_bar_height" format="dimension" />

        <attr name="progress_text_size" format="dimension" />
        <attr name="progress_text_color" format="color" />

        <attr name="progress_text_offset" format="dimension" />

        <attr name="progress_text_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
        </attr>
    </declare-styleable>

    <!-- cordova-plugin-ImagePicker -->
    <declare-styleable name="FreeCropImageView">
        <attr name="cropMaskColor" format="color" />
        <attr name="cropBorderColor" format="color" />
        <attr name="cropBorderWidth" format="dimension" />
        <attr name="cropFocusWidth" format="dimension" />
        <attr name="cropFocusHeight" format="dimension" />
        <attr name="cropStyle" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="circle" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="IndicatorView">
        <attr name="iv_IndicatorEven" format="reference|boolean" />
        <attr name="iv_IndicatorColor" format="reference|color" />
        <attr name="iv_IndicatorColorStart" format="reference|color" />
        <attr name="iv_IndicatorColorEnd" format="reference|color" />
        <attr name="iv_IndicatorColorGradient" format="reference|boolean" />
        <attr name="iv_IndicatorTextGap" format="reference|dimension" />
        <attr name="iv_IndicatorTextArray" format="reference" />
        <attr name="iv_IndicatorTextSize" format="reference|dimension" />
        <attr name="iv_IndicatorTextColorNormal" format="reference|color" />
        <attr name="iv_IndicatorTextColorSelected" format="reference|color" />
        <attr name="iv_IndicatorLengthExtra" format="reference|dimension" />
        <attr name="iv_IndicatorDuration" format="reference|integer" />
        <attr name="iv_IndicatorSelectedIndex" format="reference|integer" />
        <attr name="iv_IndicatorHeight" format="reference|dimension" />
        <attr name="iv_IndicatorBgTouchedColor" format="reference|color" />
        <attr name="iv_IndicatorViewPagerAnim" format="reference|boolean" />
    </declare-styleable>

    <declare-styleable name="SimpleIndicatorView">
        <attr name="iv_SimpleIndicatorColor" format="reference|color" />
        <attr name="iv_SimpleIndicatorColorStart" format="reference|color" />
        <attr name="iv_SimpleIndicatorColorEnd" format="reference|color" />
        <attr name="iv_SimpleIndicatorColorGradient" format="reference|boolean" />
        <attr name="iv_SimpleIndicatorCount" format="reference|integer" />
        <attr name="iv_SimpleIndicatorWidthRation" format="reference|float" />
        <attr name="iv_SimpleIndicatorDuration" format="reference|integer" />
        <attr name="iv_SimpleIndicatorDefaultIndex" format="reference|integer" />
        <attr name="iv_SimpleIndicatorLineColor" format="reference|color" />
        <attr name="iv_SimpleIndicatorLineShow" format="reference|boolean" />
        <attr name="iv_SimpleIndicatorLineThickness" format="reference|dimension" />
        <attr name="iv_SimpleIndicatorOrientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="iv_SimpleIndicatorLinePosition" format="enum">
            <enum name="start" value="0" />
            <enum name="end" value="1" />
        </attr>
    </declare-styleable>

    /** MarqueeView **/
    <declare-styleable name="MarqueeViewStyle">
        <attr name="mvInterval" format="integer|reference" />
        <attr name="mvAnimDuration" format="integer|reference" />
        <attr name="mvTextSize" format="dimension|reference" />
        <attr name="mvTextColor" format="color|reference" />
        <attr name="mvSingleLine" format="boolean" />
        <attr name="mvFont" format="reference" />
        <attr name="mvGravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="mvDirection">
            <enum name="bottom_to_top" value="0" />
            <enum name="top_to_bottom" value="1" />
            <enum name="right_to_left" value="2" />
            <enum name="left_to_right" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="Bubble">
        <attr name="topLeftRadius" format="dimension|reference" />
        <attr name="topRightRadius" format="dimension|reference" />
        <attr name="bottomLeftRadius" format="dimension|reference" />
        <attr name="bottomRightRadius" format="dimension|reference" />
        <attr name="android:foreground" />
    </declare-styleable>

    <declare-styleable name="ScrollingBackgroundView">
        <attr name="sbv_drawable" format="reference" />
        <attr name="sbv_scrollX" format="dimension" />
        <attr name="sbv_scrollY" format="dimension" />
        <attr name="sbv_repeat_x" format="boolean" />
        <attr name="sbv_repeat_y" format="boolean" />
        <attr name="sbv_fill_width" format="boolean" />
    </declare-styleable>
</resources>
